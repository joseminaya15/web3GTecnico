/*MODAL*/
.modal
	&.fade
		.modal-dialog
			transform translate(-50% , -50%)
			position absolute
			top 50%
			left 50%
			.mdl-card__actions
				padding 16px
				button
					margin 0 5px
					text-transform initial
					font-family $medium
					&.js-button
						background-color $default !important
						text-decoration none !important
						font-size 16px
						color #FFFFFF !important
						border 2px solid $default
						width initial
						font-family $medium
		&.in
			.modal-dialog
				transform translate(1)
				transform transform(-50% , -50%)
				opacity 1
/*TABLE*/
table
	&>tbody
		&>tr
			&>td
				font-size 16px
.table-responsive
	border none
	ul
		padding 0
.js-dont-bulled
	list-style none
	li
		line-height 18px
/*TOASTR*/
/*SCROLL*/
::-webkit-scrollbar
	width 5px
	height 5px
::-webkit-scrollbar-track
	background-color transparent
	border-radius 10px
::-webkit-scrollbar-thumb
	background-color #AAAAAA
	border-radius 10px
@media (min-width: 768px)
	.navbar
		background-color transparent
		.navbar-nav
			&>li
				&>a
					color #FFFFFF
					&:hover
						color #FFFFFF
		&.navbarcolor
			background-color #FFFFFF
			.navbar-brand
				img
					max-width 38px
			.navbar-nav
				&>li
					&>a
						color #000000
	.navbar-brand
		img
			max-width 75px
@media (max-width: 1000px)
	.navbar-brand
		small
			display none
	.js-information__left
		max-width 300px
	.js-information__right
		max-width calc(100% - 300px)
	.js-contactanos
		.js-contactanos--left
			max-width 350px
			iframe
				width 350px
		.js-contactanos--right
			max-width calc(100% - 350px)
@media (max-width: 600px)
	.navbar-brand
		small
			display none	
	.js-flex--start
		display block
	.js-information__left
		max-width 100%
	.js-information__right
		max-width 100%
		padding 20px 0	
	.js-contenido
		padding 40px 20px
		h2
			font-size 42px
			line-height 42px
	.js-subtitle
		text-align center
	.js-contactanos
		display block
		.js-contactanos--left
			max-width 100%
			iframe
				width 100%
		.js-contactanos--right
			max-width 100%