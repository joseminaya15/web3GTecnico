.js-fondo
	absolute()
	background url(../img/fondo/fondo1.jpg) no-repeat center center
	background-size cover
	width 100%
	height 100vh
	z-index -1
	&:BEFORE
		absolute()
		content ''
		background-color #000000
		z-index 0
		height 100vh
		opacity .5
footer
	background-color #000000
	color #FFFFFF
	p
		font-size 14px
.navbar-default
	transitionAll()
	margin 0
	padding 10px
	border none
	border-radius 0
	position fixed
	width 100%
	background-color #FFFFFF
	z-index 10
	&.navbarcolor
		box-shadow 0 2px 5px 0 rgba(0,0,0,.16), 0 2px 10px 0 rgba(0,0,0,.12)
	.navbar-nav
		text-align center
		&>li
			&>a
				color #000000
				font-size 18px
				background-color transparent !important
	.navbar-brand
		padding 10px 0
		font-size 26px
		line-height 30px
		color #FFFFFF
		position relative
		small
			position absolute
			right 0
			margin-right -230px
			margin-top -25px
			color #FFFFFF
		img
			transitionMaxWidth()
			max-width 38px
.nav-tabs
	border none
	&>li
		&>a
			border none !important
			font-size 18px
	&>li
		&.active
			&>a
				font-family $bold				
/*CLASES PROYECTO*/
