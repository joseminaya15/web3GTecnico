	width 100%
	max-width calc(100% - 500px)
	p
		margin 5px 0
.js-contactanos
	flexStart()
	.js-contactanos--left
		width 100%
		max-width 500px
	.js-contactanos--right
		flexStart()
		padding 20px
		justify-content flex-start
		flex-direction column
		width 100%
		max-width calc(100% - 500px)
		.js-flex
			margin 10px
			i
				margin-right 10px
			a
				color #000000
				text-decoration none
				font-size 16px
.js-card
	background-color #FFFFFF
	box-shadow 0 2px 5px rgba(0,0,0,0.16), 0 2px 5px rgba(0,0,0,0.23)
	padding 20px
.js-partners
	img
		max-width 250px
		margin 15px
#logistic
	background-color #614767
#register
	background-color #90A878
.js-book
	background-color #614767 !important
	text-decoration none !important
