		a
			color #008FD3 !important
	.mdl-checkbox__label
		font-size 14px
		color #000000
		line-height 18px
		font-family $regular
		font-weight 100
#home
	overflow hidden
	padding 0
	position relative
	height 100vh
	.owl-carousel
		relative()
		height 100vh
		width 100%
		z-index 2
		.owl-item
			height 100vh
			.item
				height 100vh
				.fondo-responsive
					position absolute
					top 0
					right 0
					bottom 0
					left 0
